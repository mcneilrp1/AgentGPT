{
  "i18n-ally.localesPaths": ["public/locales"],
  "i18n-ally.keystyle": "flat",
  "i18n-ally.sourceLanguage": "en",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "vividus.scenario.keyword",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#00f3ef"
        }
      },
      {
        "scope": "vividus.scenario.title",
        "settings": {
          "foreground": "#94d6fc"
        }
      },
      {
        "scope": "vividus.comment",
        "settings": {
          "foreground": "#1eff008e",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "vividus.meta",
        "settings": {
          "foreground": "#ffe600",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "vividus.meta.key",
        "settings": {
          "foreground": "#d6c316",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "vividus.meta.value",
        "settings": {
          "foreground": "#ff0095",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "vividus.step.type",
        "settings": {
          "foreground": "#12b6b3",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "vividus.step.wording",
        "settings": {
          "foreground": "#c47332f6"
        }
      },
      {
        "scope": [
          "vividus.keyword"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#a37be1"
        }
      },
      {
        "scope": [
          "vividus.keyword.attribute"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#a8b9fb"
        }
      },
      {
        "scope": [
          "vividus.log.time"
        ],
        "settings": {
          "foreground": "#666f76"
        }
      },
      {
        "scope": [
          "vividus.log.thread"
        ],
        "settings": {
          "foreground": "#3a86c3"
        }
      },
      {
        "scope": [
          "vividus.log.level.info"
        ],
        "settings": {
          "foreground": "#30a815"
        }
      },
      {
        "scope": [
          "vividus.log.level.error"
        ],
        "settings": {
          "foreground": "#c50f09"
        }
      },
      {
        "scope": [
          "vividus.log.level.debug"
        ],
        "settings": {
          "foreground": "#1ab288"
        }
      },
      {
        "scope": [
          "vividus.log.class"
        ],
        "settings": {
          "foreground": "#ddc728"
        }
      },
      {
        "scope": [
          "vividus.log.message"
        ],
        "settings": {
          "foreground": "#d4dbdd"
        }
      }
    ]
  },
  "yaml.customTags": [
    "!upload scalar",
    "!remove scalar",
    "!keep scalar",
    "!erase scalar",
    "!jwt scalar"
  ],
  "yaml.schemas": {
    "https://raw.githubusercontent.com/doanthuanthanh88/testapi6/main/schema.json": "*.yaml"
  }
}
